{
    "manifest_version": 3,
    "name": "Resource Manager",
    "description": "A minimal example of a chrome extension using manifest v3",
    "version": "0.0.1",
    "action": {
        "default_popup": "index.html",
        "default_width": 1000,
        "default_height": 400
    },
    "permissions": ["tabs"]
}